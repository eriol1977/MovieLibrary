<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>com.fb.movielibrary.resources.labels</base-name>
            <var>lbl</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>/rest/rottenTomatoes.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>favorites</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>findmovies</from-outcome>
            <to-view-id>/rest/rottenTomatoes.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
